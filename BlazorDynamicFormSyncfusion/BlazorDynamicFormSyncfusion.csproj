<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/TheSmallPixel/BlazorDynamicForm</PackageProjectUrl>
	<VersionPrefix>1.0.4</VersionPrefix>
    <PackageTags>Blazor;Dynamic;Form;Syncfusion;Validation;</PackageTags>
    <RepositoryUrl>https://github.com/TheSmallPixel</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright> Lorenzo Longiave(c) 2023</Copyright>
	<PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<Title>Blazor Dynamic Form Generator</Title>
	<Authors>Lorenzo Longiave</Authors>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU'">
    <PackageReference Include="BlazorDynamicFormDataAnnotation" Version="1.0.4" />
  </ItemGroup>
    
  <ItemGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <ProjectReference Include="..\BlazorDynamicFormDataAnnotation\BlazorDynamicFormDataAnnotation\BlazorDynamicFormDataAnnotation.csproj" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Syncfusion.Blazor" Version="21.2.10" />
  </ItemGroup>



</Project>
