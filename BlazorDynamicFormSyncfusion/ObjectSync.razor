@using Syncfusion.Blazor.Navigations
@using BlazorDynamicForm.Components

@inherits FormComponentBase

@if (Formmap != null && Formkey != null)
{
    <SfAccordion>
        <AccordionItems>
            <AccordionItem>
                <HeaderTemplate>
                    @Formkey
                    <div></div>
                </HeaderTemplate>
                <ContentTemplate>
                    @{
                        foreach (var nice in ChildBuilder.Invoke(Formkey, Value))
                        {
                            @nice
                        }
                    }
                </ContentTemplate>
            </AccordionItem>
        </AccordionItems>
    </SfAccordion>
}

